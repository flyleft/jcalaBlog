#include("/templates/module/header.vm")
<div class="container">
    <div class="row">
        <div class="col-lg-12 post">
            <div class="archive">
                    <h2 class="year">${tagName}</h2>
                    #foreach($bv in ${views})
                        <div class="post-item">
                            <div class="post-time">
                                $!{bv.MonthDay}
                            </div>
                            <a href="/post/$!{bv.Vid}" class="post-link">$!{bv.Title}</a>
                        </div>
                    #end
                    <br><br>
            </div>
        </div>
    </div>
</div>
#include("/templates/module/footer.vm")
